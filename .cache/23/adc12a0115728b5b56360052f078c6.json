ath.floor(Math.random() * words.length);\n    return words[randomIndex];\n  }\n\n  onSpeechResult = (): void => {};\n\n  onGameStartClick = (): void => {\n    const computerAnswer = this.pickRandomWord();\n  };\n\n  onPlayAgainClick = (): void => {\n    const newGame = Game.build({\n      level: Level.Easy,\n      answers: [],\n    });\n    const gameInitUI = new GameInitUI(this.parent, newGame);\n    gameInitUI.render();\n  };\n\n  template(): string {\n    return `\n    <div> \n      <button class=\"play-again-btn\">Play Again</button>\n      <h1 id=\"title\">Word Play</h1>\n      <input type=\"text\" value=\"-\" name=\"user-input\" id=\"user-input\" />\n      <br />\n      <button id=\"game-start-btn\">Start Game</button>\n      <br />\n      <input type=\"text\" value=\"-\" name=\"computer-input\" id=\"computer-input\" />\n      <ol id=\"user-answers-list\"></ol>\n      <ol id=\"computer-answers-list\"></ol>\n    </div>`;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"ecabd08f61f912821ec8e3bb7a061934","cacheData":{"env":{}}}